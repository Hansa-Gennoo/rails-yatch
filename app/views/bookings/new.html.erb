<%= form_with model: @booking do |f| %>
  <%= f.label :start_date %>
  <%= f.date_field :start_date %>

  <%= f.label :end_date %>
  <%= f.date_field :end_date %>

  <%= f.label :yacht_id %>
  <%= f.select :yacht_id, options_for_select(current_user.yachts.map{ |y| [y.name, y.id] }), {}, { prompt: 'Select Yacht' } %>

  <%= f.submit %>
<% end %>
